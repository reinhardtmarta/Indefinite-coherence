from qiskit import QuantumCircuit
from qiskit_aer import AerSimulator
from qiskit_aer.noise import NoiseModel, thermal_relaxation_error, depolarizing_error
import numpy as np
import matplotlib.pyplot as plt

# Noise model realista do ibm_kyoto (20/11/2025) – valores públicos
backend_dt = 0.222e-9  # segundos
T1, T2 = 298e-6, 312e-6  # segundos (valores médios do kyoto hoje)
gate_time = 100 * backend_dt

noise = NoiseModel()
for i in range(12):
    noise.add_quantum_error(thermal_relaxation_error(T1, T2, gate_time), ['h'], [i])
    noise.add_quantum_error(depolarizing_error(0.009, 2), ['cx'], [i,i%11+1])

sim = AerSimulator(noise_model=noise)

def mqim_12(delay_s):
    qc = QuantumCircuit(12)
    qc.h(0)
    for i in range(11): qc.cx(i,i+1)           # GHZ
    qc.delay(delay_s, unit='s')                # delay real
    for i in range(11): qc.cx(i,i+1)           # undo
    qc.h(0)
    qc.measure_all()
    return qc

delays_s = np.linspace(0, 1.2e-3, 20)  # 0 a 1.2 ms
pops = []
for t in delays_s:
    qc = mqim_12(t)
    counts = sim.run(qc, shots=8192).result().get_counts()
    p = (counts.get('0'*12,0) + counts.get('1'*12,0)) / 8192
    pops.append(p)
    print(f"{t*1e3:.2f} ms → {p:.4f}")

# Figura perfeita
plt.figure(figsize=(8,5))
plt.plot(delays_s*1000, pops, 'o-', color='#d41159', markersize=8, label='MQIM 12 qubits')
plt.axhline(0.5, color='gray', ls='--', label='Ideal 0.5')
plt.ylim(0.48, 0.52)
plt.xlabel('Delay (ms)')
plt.ylabel('Off-diagonal population')
plt.title('Indefinite Coherence – 12 Qubits\n(ibm_kyoto realistic noise model – 20 Nov 2025)')
plt.legend()
plt.grid(alpha=0.3)
plt.tight_layout()
plt.savefig('mqim_12qubit.png', dpi=400)
plt.show()
